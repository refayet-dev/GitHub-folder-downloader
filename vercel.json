{
    "version": 2,
    "name": "GDM",
    "builds": [
        {
            "src": "./package.json",
            "use": "@vercel/static-build",
            "config": { 
                "distDir": "./static",
                "buildCommand": "npm run build",
                "outputDirectory": "dist"
             }
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "./static/index.html" }
    ]
}